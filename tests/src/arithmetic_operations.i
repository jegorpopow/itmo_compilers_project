routine main() is
  var a is 7;
  var b is 3;
  var x is 5.5;
  var y is 2.0;
  
  print a + b;
  print a - b;
  print a * b;
  print a / b;
  print a % b;
  
  print (-7) / 3;
  print (-7) % 3;
  print 7 / (-3);
  print 7 % (-3);
  
  print x + y;
  print x - y;
  print x * y;
  print x / y;
  
  print a + x;
  print x + b;
  print a * y;
  print y - a;
  
  print -a;
  print -x;
end;

